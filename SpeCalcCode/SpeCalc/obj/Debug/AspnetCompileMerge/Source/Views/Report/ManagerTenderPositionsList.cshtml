@model IEnumerable<SpeCalc.Models.Tender>
           
@if (Model.Any())
{
    <table>
        <tr>
            <th>№</th>
            <th>Менеджер</th>
            <th>Количество позиций</th>
            <th>Количество расчетов</th>
        </tr>
        @foreach (var ten in Model)
        {
            <tr>
                <td>@ten.Manager.DisplayName</td>
                <td>@ten.PositionCount</td>
                <td>@ten.CalcCount</td>
            </tr>
        }
    </table>
}
